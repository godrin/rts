<?xml version="1.0"?>
<layout name="QuitDialog" geometry2="(0,0,640,480)">
	<table cols="3" rows="3"><!-- center window -->
		<colsize col="1" fixed="300"/>
		<rowsize row="1" fixed="100"/>
		<window col="1" row="1" name="window">
			<table cols="1" rows="2">
				<colsize col="0" relative="1"/>
				<rowsize row="0" relative="1"/>
				<rowsize row="1" fixed="30"/>
				<text col="0" row="0" name="text" caption="Do you really want\nto quit the game?" align="center"/>
				<table col="0" row="1" cols="2" rows="1">
					<button col="0" row="0" name="ok" theme="antButton" caption="Ok"/>
					<button col="1" row="0" name="cancel" theme="antButton" caption="Cancel"/>
				</table>
			</table>
		</window>
	</table>
</layout>
